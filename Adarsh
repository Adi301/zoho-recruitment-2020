public class MyClass {
    public static void main(String args[]) {
        String s="welcome";
      int l=s.length(), mid;
      
	    for (int i=0;i<l;i++){
	        
	       String md="";
	   
	       mid=l/2;
	       
	       for (int j=0;j<=i;j++)
	       
	       
	       md+=s.charAt((mid++)%l);
	      
	       System.out.println(" "+md);
	       }
    }
}
